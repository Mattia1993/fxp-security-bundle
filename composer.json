{
    "name": "sonatra/security-bundle",
    "type": "symfony-bundle",
    "description": "Implements all features of Symfony2 Advanced ACL Concepts and adds some interesting features",
    "keywords": ["ACL", "ACE", "Advanced ACL Concept Implementation", "Rule definition", "Sharing rule", "ACL Voter", "JMS Expression"],
    "homepage": "https://github.com/sonatra/SonatraSecurityBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/security-bundle": "~3.0",
        "sonatra/security": "~1.0@dev"
    },
    "require-dev": {
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "doctrine/doctrine-bundle": "~1.6",
        "jms/security-extra-bundle": "~1.6",
        "doctrine/orm": "~2.5"
    },
    "suggest": {
        "jms/security-extra-bundle": "For JMS Security Extra Expression features"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\SecurityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
