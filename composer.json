{
    "name": "sonatra/security-bundle",
    "type": "symfony-bundle",
    "description": "A Role-Based Access Control Level 2 with advanced permissions and sharing rules",
    "keywords": ["RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/sonatra/sonatra-security-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "~3.1",
        "symfony/security-bundle": "~3.1",
        "sonatra/security": "~1.0@dev"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "~3.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/orm": "~2.5"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\SecurityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
