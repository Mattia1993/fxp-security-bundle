{
    "name": "sonatra/security-bundle",
    "type": "symfony-bundle",
    "description": "A Extended Role-Based Access Control with sharing rules",
    "keywords": ["RBAC", "E-RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/sonatra/sonatra-security-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "symfony/framework-bundle": "^3.2.0",
        "symfony/security-bundle": "^3.2.0",
        "sonatra/security": "^1.0.0"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "^3.0.0",
        "friendsofsymfony/user-bundle": "^2.0.0",
        "doctrine/doctrine-bundle": "^1.6.0",
        "doctrine/orm": "^2.5.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\SecurityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
